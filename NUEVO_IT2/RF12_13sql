--RF12
SELECT ID 
FROM SERVICIO
WHERE HORARIO BETWEEN Convert(datetime, '2020-03-01' ) AND Convert(datetime, '2020-04-01' );

UPDATE SERVICIO
SET "ESTADO" = "NO_DISPONIBLE"
WHERE ID = 205;

UPDATE SERVICIO
SET "ESTADO" = "RESERVADOCAMPAÑA";
WHERE ID = 205;

UPDATE SERVICIO
SET "ESTADO" = "RESERVADOPACIENTE";
WHERE ID = 205;

--RF11
UPDATE SERVICIO
SET "ESTADO" = "DISPONIBLE";
WHERE ID = 205;
